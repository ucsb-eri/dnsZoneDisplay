<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p>Zone Directory (from config.json): <%= conf.zonedir %></p>
    <% files.forEach(function(file) { %>
      <a href="/zone/<%= file %>"><%= file %></a> <br>
    <% }); %>
  </body>
</html>
